<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
  <configuration>
  	<typeAliases>
		<typeAlias alias = "MyMember" type="bean.Member"/>
		<typeAlias alias = "MyBoard" type="bean.Board"/>
		<typeAlias alias = "MyComments" type="bean.Comments"/>
		<typeAlias alias = "MyAlbum" type="bean.Album"/>
		<typeAlias alias = "MySong" type="bean.Song"/>
  	</typeAliases>
  	
  	
  	<mappers>
		<mapper resource="mybatis/mapmember.xml"/>
		<mapper resource="mybatis/mapboard.xml"/>
