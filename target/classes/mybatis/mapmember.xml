<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
	<mapper namespace="NsMember">
		<select id="SelectById" resultType="MyMember" parameterType="HashMap">
			select * from member where mid = #{id} and password = #{password}
		</select>
		
		<insert id="SiginInData" parameterType="MyMember">
			insert into member
			values(#{mid}, #{password}, #{name}, #{age}, #{phone}, #{email}, #{postcode}, #{address1}, #{address2})
		</insert>
	</mapper>
	
	